{
  "version": 2,
  "routes": [
    {
      "src": "/api/login",
      "methods": ["POST"],
      "dest": "/api/login.js"
    },
    {
      "src": "/api/register",
      "methods": ["POST"],
      "dest": "/api/register.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/login.js": {
      "memory": 128,
      "maxDuration": 10
    },
    "api/register.js": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}